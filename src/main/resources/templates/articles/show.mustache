{{>layouts/header}}


<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">TITLE</th>
        <th scope="col">CONTENT</th>

    </tr>
    </thead>

    <tbody>

    {{#article}}

    <tr>
        <td>{{id}}</td>
        <td>{{title}}</td>
        <td>{{content}}</td>
    </tr>

    {{/article}}

    </tbody>
</table>
<a href="/articles/{{article.id}}/edit" class="btn btn-primary">Edit</a>
<a href="/articles/{{article.id}}/delet" class="btn btn-danger">Delete</a>
<a href="/articles" >Go to Article List</a>

{{>comments/_comments}}

{{>layouts/footer}}